<actions>
    <action>
        <actionName>run</actionName>
        <packagings>
            <packaging>war</packaging>
            <packaging>ear</packaging>
            <packaging>ejb</packaging>
        </packagings>
        <goals>
            <goal>package</goal>
            <goal>org.codehaus.mevenide:netbeans-deploy-plugin:LATEST:deploy</goal>
        </goals>
    </action>
    <action>
        <actionName>run.single</actionName>
        <packagings>
            <packaging>war</packaging>
        </packagings>
        <goals>
            <goal>package</goal>
            <goal>org.codehaus.mevenide:netbeans-deploy-plugin:LATEST:deploy</goal>
        </goals>
        <properties>
            <netbeans.clientUrlPart>${webpagePath}</netbeans.clientUrlPart>
        </properties>
    </action>
    <action>
        <actionName>debug</actionName>
        <packagings>
            <packaging>war</packaging>
            <packaging>ear</packaging>
            <packaging>ejb</packaging>
        </packagings>
        <goals>
            <goal>package</goal>
            <goal>org.codehaus.mevenide:netbeans-deploy-plugin:LATEST:deploy</goal>
        </goals>
        <properties>
            <netbeans.deploy.debugmode>true</netbeans.deploy.debugmode>
        </properties>
    </action>
    <action>
        <actionName>debug.single</actionName>
        <packagings>
            <packaging>war</packaging>
        </packagings>
        <goals>
            <goal>package</goal>
            <goal>org.codehaus.mevenide:netbeans-deploy-plugin:LATEST:deploy</goal>
        </goals>
        <properties>
            <netbeans.clientUrlPart>${webpagePath}</netbeans.clientUrlPart>
            <netbeans.deploy.debugmode>true</netbeans.deploy.debugmode>
        </properties>
    </action>
    
</actions>
