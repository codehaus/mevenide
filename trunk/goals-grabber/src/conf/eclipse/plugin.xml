<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="@maven.eclipse.plugin.artifact.id@"
   name="Mevenide Goals Grabber"
   version="@maven.eclipse.plugin.artifact.version@"
   provider-name="Sourceforge">

   <runtime>
	   <library name="lib/commons-collections-2.1.jar">
         	<export name="*"/>
         	<packages prefixes="org.apache.commons.collections"/>
         </library>
         <library name="@maven.eclipse.plugin.artifact.internal.id@-@maven.eclipse.plugin.artifact.internal.version@.jar">
         	<export name="*"/>
         	<packages prefixes="org.mevenide.goals"/>
         </library>
   </runtime>
   <requires>
      <import plugin="@org.mevenide.core.id@"/>
   </requires>


</plugin>
