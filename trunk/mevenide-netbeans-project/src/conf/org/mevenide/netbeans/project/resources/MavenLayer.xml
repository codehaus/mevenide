<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Project">
            <!-- XXX why are these here? should be moved elsewhere -->
            <attr name="Standard/Maven" boolvalue="true"/>

            <folder name="Maven">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
                <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/mevenide/netbeans/resources/maven_projects.html"/>

                <file name="empty.zip" url="empty.zip">
                    <attr name="template" boolvalue="true"/>
                    <attr name="instantiatingIterator" newvalue="org.mevenide.netbeans.project.wizards.MavenNewWizardIterator"/>
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/MavenIcon.gif"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/emptyProject.html"/>
                </file>
                <file name="mavenplugin.zip" url="mavenplugin.zip">
                    <attr name="template" boolvalue="true"/>
                    <attr name="instantiatingIterator" newvalue="org.mevenide.netbeans.project.wizards.MavenNewWizardIterator"/>
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/MavenIcon.gif"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/mavenpluginProject.html"/>
                </file>
                <file name="nbmodule.zip" url="nbmodule.zip">
                    <attr name="template" boolvalue="true"/>
                    <attr name="instantiatingIterator" newvalue="org.mevenide.netbeans.project.wizards.MavenNewWizardIterator"/>
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/MavenIcon.gif"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/nbmoduleProject.html"/>
                </file>
            </folder>
        </folder>
        
        <attr name="API_Support/MavenDocumentation" boolvalue="true"/>        
        <attr name="MavenDocumentation/Other" boolvalue="true"/>        
        <folder name="MavenDocumentation">
           <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
           <file name="xdoc.xml" url="templates/xdoc.xml">
             <attr name="template" boolvalue="true" />
             <attr name="templateWizardURL" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/templates/XDoc.html" />
             <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
             <attr name="templateCategory" stringvalue="maven-docs"/>
          </file>
           <attr name="xdoc.xml/faq.fml" boolvalue="true"/>        
           <file name="faq.fml" url="templates/faq.fml">
             <attr name="template" boolvalue="true" />
             <attr name="templateWizardURL" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/templates/FAQ.html" />
             <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
             <attr name="templateCategory" stringvalue="maven-docs"/>
          </file>
           <attr name="faq.fml/changes.xml" boolvalue="true"/>        
           <file name="changes.xml" url="templates/changes.xml">
             <attr name="template" boolvalue="true" />
             <attr name="templateWizardURL" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/templates/Changes.html" />
             <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.resources.Bundle"/>
             <attr name="templateCategory" stringvalue="maven-docs"/>
          </file>
       </folder>
        
    </folder>



    <folder name="Plugins">
        <folder name="Maven">
            <folder name="Properties">
                <file name="defaults.properties" url="maven_defaults.properties"/>
            </folder>
            <folder name="PluginProperties">
                <file name="maven-jar-plugin-1.6.1.xml" url="plugins/maven-jar-plugin-1.6.1.xml"/>
                <file name="maven-java-plugin-1.5.xml" url="plugins/maven-java-plugin-1.5.xml"/>
                <file name="maven-pde-plugin-0.3.xml" url="plugins/maven-pde-plugin-0.3.xml"/>
            </folder>
        </folder>
    </folder>
 
    
  <folder name="Services">
    <file name="org-mevenide-netbeans-project-MavenSettings.settings" 
          url="MavenSettings.xml">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.netbeans.project.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/netbeans/project/resources/MavenIcon.gif"/>
    </file>
  </folder>
  <folder name="UI">
    <folder name="Services">
      <folder name="Building">
        <file name="org-mevenide-netbeans-project-MavenSettings.shadow">
          <attr name="originalFile" stringvalue="Services/org-mevenide-netbeans-project-MavenSettings.settings"/>
        </file>
      </folder> <!-- Building -->
    </folder> <!-- Services -->
  </folder>   
  <!-- Lines marked as erroneous: -->
  <folder name="Editors">
    <folder name="AnnotationTypes">
      <file name="org-mevenide-netbeans-project-error.xml" url="error-annotation.xml"/>
      <file name="org-mevenide-netbeans-project-pmd.xml" url="pmd-annotation.xml"/>
      <file name="org-mevenide-netbeans-project-checkstyle.xml" url="checkstyle-annotation.xml"/>
      <folder name="StatusForAnnotationType">
          <file name="org-mevenide-netbeans-project-checkstyle">
              <attr name="status" stringvalue="warning" />
              <attr name="browseable" boolvalue="true" />
              <attr name="color" stringvalue="0xFFDF80" />
          </file>
          <file name="org-mevenide-netbeans-project-pmd">
              <attr name="status" stringvalue="warning" />
              <attr name="browseable" boolvalue="true" />
              <attr name="color" stringvalue="0xFFBF80" />
          </file>
          <file name="org-mevenide-netbeans-project-error">
              <attr name="status" stringvalue="error" />
              <attr name="browseable" boolvalue="true" />
              <attr name="color" stringvalue="0xFF8080" />
          </file>
      </folder>
    </folder>
  </folder>    
</filesystem>
