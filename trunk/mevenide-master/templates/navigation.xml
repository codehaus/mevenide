<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Mevenide Project" 
	     href="http://mevenide.sf.net/">

  <title>Mevenide</title>

  <body>
  	<links>
      <item href="http://maven.apache.org/" name="Maven"/>
      <item href="http://mevenide.sf.net/" name="Mevenide"/>
    </links>
  	
    <menu name="Home">
      <item name="About" href="/index.html" />
      <item name="Source Repositories" href="/source-repos.html" />
    </menu>

    <menu name="Sub Projects">
      #foreach ($reactorProject in $reactorProjects)
        #set ( $project = $reactorProject.name )
        #set ( $master = $project.endsWith("Master") )
        #if ( !$master )
          <item name="${reactorProject.name}" 
          	    href="${mevenide.site.prefix}${reactorProject.siteDirectory}/index.html"/>
        #end
      #end
    </menu>

  </body>
</project>

