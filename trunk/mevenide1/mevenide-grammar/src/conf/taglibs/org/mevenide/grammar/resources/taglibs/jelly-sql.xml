<?xml version="1.0" encoding="UTF-8"?>

<!-- code completion definition for the jelly-tag library -->

<taglib name="jelly:sql">
    <tag name="dateParam">
        <attr name="type"/>
        <attr name="value"/>
        <attr name="trim"/>
    </tag>
    <tag name="driver">
        <attr name="driver"/>
        <attr name="jdbcURL"/>
        <attr name="scope"/>
        <attr name="userName"/>
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
    <tag name="param">
        <attr name="value"/>
        <attr name="trim"/>
    </tag>
    <tag name="query">
        <attr name="dataSource"/>
        <attr name="maxRows"/>
        <attr name="scope"/>
        <attr name="sql"/>
        <attr name="startRow"/>
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
    <tag name="resultSet">
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
    <tag name="row" within="resultSet">
        <attr name="trim"/>
   </tag>
    <tag name="setDataSource">
         <attr name="dataSource"/>
         <attr name="driver"/>
         <attr name="password"/>
         <attr name="scope"/>
         <attr name="url"/>
         <attr name="user"/>
         <attr name="var"/>
         <attr name="trim"/>
   </tag>
    <tag name="update">
        <attr name="dataSource"/>
        <attr name="scope"/>
        <attr name="sql"/>
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
</taglib>
