<?xml version="1.0" encoding="UTF-8"?>
<site>
	<description url="http://mevenide.codehaus.org/mevenide-ui-eclipse/update/index.html">
		%siteDescription
	</description>

	<feature url="features/org.mevenide.feature_0.2.0.jar" id="org.mevenide.feature" version="0.2.0">
		<category name="Mevenide"/>
	</feature>
	
	<feature url="features/@org.mevenide.eclipse.feature.id@_@org.mevenide.eclipse.feature.version@.jar" id="@org.mevenide.eclipse.feature.id@" version="@org.mevenide.eclipse.feature.version@">
		<category name="Mevenide"/>
	</feature>

	<feature url="features/org.apache.maven_1.0.0.4.jar" id="org.apache.maven" version="1.0.0.4">
		<category name="Mevenide"/>
	</feature>

	<category-def label="Mevenide" name="Mevenide">
		<description>
			%mevenideCategoryDescription
		</description>
	</category-def>
	
	<category-def label="Maven" name="Maven">
		<description>
			%mavenCategoryDescription
		</description>
	</category-def>

</site>