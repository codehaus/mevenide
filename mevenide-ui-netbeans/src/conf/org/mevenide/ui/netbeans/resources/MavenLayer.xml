<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">
<filesystem>
  <folder name="Templates">
    <folder name="Services">
        <folder name="Executor">
            <file name="org-mevenide-ui-netbeans-exec-MavenExecutor.settings" 
                  url="MavenExecutor.settings">
                <attr boolvalue="true" name="template"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org-mevenide-ui-netbeans.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/ui/netbeans/resources/MavenIcon.gif"/>
            </file>
        </folder>
    </folder>
    
    <folder name="Maven">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.ui.netbeans.resources.TemplateNames"/>
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/mevenide/ui/netbeans/resources/templates/Maven.html"/>
        
        <file name="SampleProject.group" url="templates/SampleProject.group">
          <attr name="template" boolvalue="true"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.ui.netbeans.resources.TemplateNames"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/mevenide/ui/netbeans/resources/templates/SampleProject.html"/>
          <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/ui/netbeans/resources/MavenIcon.gif"/>
        </file>
        <file name="project.properties" url="nbresloc:/org/mevenide/ui/netbeans/resources/templates/project.properties"/>
        <file name="maven.xml" url="nbresloc:/org/mevenide/ui/netbeans/resources/templates/maven.xml"/>
        <file name="project.xml" url="nbresloc:/org/mevenide/ui/netbeans/resources/templates/project.xml">
<!--          <attr name="NetBeansAttrExecutor" methodvalue="org.netbeans.modules.apisupport.APIModule.lookupSystemActionTester"/> -->
        </file>        
        
      </folder>

  </folder>

  <folder name="Services">

    <folder name="Executor">
        <file name="org-mevenide-ui-netbeans-exec-MavenExecutor.settings" 
              url="MavenExecutor.settings">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.ui.netbeans.Bundle"/>
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/ui/netbeans/resources/MavenIcon.gif"/>
            <attr name="SystemFileSystem.layer" stringvalue="project"/>
        </file>
    </folder>

    <file name="org-mevenide-ui-netbeans-MavenSettings.settings" 
          url="MavenSettings.settings">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.ui.netbeans.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/ui/netbeans/resources/MavenIcon.gif"/>
    </file>

    <!-- Recognize text/x-+xml: -->
    <folder name="MIMEResolver">
      <file name="org-mevenide-ui-netbeans-mime-resolver.xml" url="mime-resolver.xml">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.mevenide.ui.netbeans.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/mevenide/ui/netbeans/resources/MavenIcon.gif"/>
      </file>
    </folder>

    <folder name="Hidden">
      <!-- To make the module nicer to debug, list the cookies it provides. -->
<!--TODO      <file name="org-mevenide-ui-netbeans-cookies.xml" url="cookies.xml"/> -->
    </folder>
  </folder>

  <folder name="UI">
    <folder name="Services">
      <folder name="Building">
        <file name="org-mevenide-ui-netbeans-MavenSettings.shadow">
          <attr name="originalFile" stringvalue="Services/org-mevenide-ui-netbeans-MavenSettings.settings"/>
        </file>
      </folder> <!-- Building -->
    </folder> <!-- Services -->
  </folder> <!-- UI -->
  
</filesystem>
