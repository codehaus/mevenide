<project>
  <extend>${basedir}/../mevenide-parent/project.xml</extend>
  <pomVersion>3</pomVersion>
  <name>Mevenide Master</name>
  <id>mevenide-master</id>
  <artifactId>mevenide-master</artifactId>
  <currentVersion>0.3</currentVersion>
  <url>http://mevenide.codehaus.org/</url>
  
  <siteDirectory>/home/projects/mevenide/public_html</siteDirectory>
  
  <dependencies>
  	<dependency>
  	  <groupId>commons-jelly</groupId>
      <artifactId>commons-jelly-tags-xml</artifactId>
  	  <version>1.1</version>
  	</dependency>
  	<dependency>
  	  <groupId>maven</groupId>
      <artifactId>maven-dashboard-plugin</artifactId>
  	  <version>1.0</version>
  	  <type>plugin</type>
  	</dependency>
  	<dependency>
  	  <groupId>mevenide</groupId>
      <artifactId>maven-eclipse-plugin-plugin</artifactId>
  	  <version>0.4.0</version>
  	  <type>plugin</type>
  	</dependency>
  	<dependency>
  	  <groupId>mevenide</groupId>
      <artifactId>maven-nbm-plugin</artifactId>
  	  <version>1.5</version>
  	  <type>plugin</type>
  	</dependency>
   
  </dependencies>
  
  <reports>
    <report>maven-changes-plugin</report> 
    <report>maven-faq-plugin</report> 
    <report>maven-multiproject-plugin</report> 
  </reports>
 
</project>

