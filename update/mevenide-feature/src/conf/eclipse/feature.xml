<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="@maven.eclipse.plugin.artifact.id@"
      label="Mevenide, Maven plugin for Eclipse"
      version="@maven.eclipse.plugin.artifact.version@"
      provider-name="Sourceforge"
      plugin="Mevenide"
      image="logo.jpg">

   <description>
      %description
   </description>

   <license url="http://mevenide.sourceforge.net/docs/mevenide-ui-eclipse/license.html">
      %license
   </license>

   <url>
      <update label="Mevenide updates" url="http://mevenide.sourceforge.net/updates/"/>
   </url>
	
   <requires>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.swt"/>
      <import plugin="org.eclipse.ui.workbench"/>
      <import plugin="org.eclipse.jdt.launching"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.externaltools"/>
      <import plugin="org.eclipse.jface.text"/>
   </requires>

   <plugin id="@org.mevenide.core.id@"
           download-size="0"
           install-size="0"
           version="@org.mevenide.core.version@"/>

   <plugin id="@org.mevenide.grabber.id@"
           download-size="0"
           install-size="0"
           version="@org.mevenide.grabber.version@"/>

   <plugin id="@org.mevenide.ui.eclipse.id@"
           download-size="0"
           install-size="0"
           version="@org.mevenide.ui.eclipse.version@"/>

</feature>



