<?xml version="1.0" encoding="UTF-8"?>

<project>
  <pomVersion>3</pomVersion>
  <id>jdom-nb-module</id>
  <artifactId>jdom-nb-module</artifactId>
  <groupId>mevenide</groupId>
  <currentVersion>1.0b8</currentVersion>
  <package>jdom</package>
  <name>jdom library for Mevenide.</name>
  <shortDescription>Library for project mevenide</shortDescription>
  <description>Library for project mevenide</description>
  <url>http://mevenide.sf.net</url>
  <dependencies>
    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>1.0b8</version>
      <type>jar</type>
      <properties>
        <netbeans.module.ext>true</netbeans.module.ext>
      </properties>
    </dependency>
  </dependencies>
  <build>
    <resources>
      <resource>
        <directory>src/conf</directory>
        <includes>
          <include>**</include>
        </includes>
        <excludes>
          <exclude>**/.nbattrs</exclude>
          <exclude>manifest.mf</exclude>
        </excludes>
      </resource>
    </resources>
  </build>  
</project>