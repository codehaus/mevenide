<?xml version="1.0"?>
<project>
  <extend>${basedir}/../mevenide-parent/project.xml</extend>
  <pomVersion>3</pomVersion>
  <name>JBuilder Maven OpenTool</name>
  <id>mevenide-ui-jbuilder</id>
  <artifactId>mevenide-ui-jbuilder</artifactId>
  <currentVersion>0.2</currentVersion>
  <inceptionYear>2004</inceptionYear>
  <package>org.mevenide</package>
  <shortDescription>OpenTool JBuilder Maven IDE Plugin</shortDescription>
  <description>
      This project integrates Maven with the JBuilder IDE.
  </description>
  <versions>
    <version>
      <id>0.1</id>
      <name>0.1</name>
      <tag>V_0_1_0</tag>
    </version>
    <version>
      <id>0.2</id>
      <name>0.2</name>
      <tag>V_0_2_0</tag>
    </version>
  </versions>

  <dependencies>
    <dependency>
      <groupId>mevenide</groupId>
      <artifactId>mevenide-core</artifactId>
      <version>0.10</version>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>mevenide</groupId>
      <artifactId>goals-grabber</artifactId>
      <version>0.10</version>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>mevenide</groupId>
      <artifactId>mevenide-config</artifactId>
      <version>0.9</version>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
        <jbuilder.plugin.autoupdate>false</jbuilder.plugin.autoupdate>
      </properties>
    </dependency>

    <!-- inherited-dependencies -->
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>2.1</version>
      <type>jar</type>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.1</version>
      <type>jar</type>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-lang</groupId>
      <artifactId>commons-lang</artifactId>
      <version>1.0.1</version>
      <type>jar</type>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>xpp3</groupId>
      <artifactId>xpp3</artifactId>
      <version>1.1.2a</version>
      <type>jar</type>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.7</version>
      <url>http://jakarta.apache.org/log4j/</url>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>maven</groupId>
      <artifactId>maven</artifactId>
      <version>1.0.1</version>
      <type>jar</type>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>ant</groupId>
      <artifactId>ant</artifactId>
      <version>1.5.3-1</version>
      <type>jar</type>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <properties>
        <jbuilder.opentool.bundle>true</jbuilder.opentool.bundle>
      </properties>
    </dependency>

    <!-- JBuilder Dependencies -->
    <dependency>
      <groupId>jbuilder</groupId>
      <artifactId>jbuilder</artifactId>
      <version>2005</version>
    </dependency>
    <dependency>
      <groupId>jbuilder</groupId>
      <artifactId>help</artifactId>
      <version>2005</version>
    </dependency>
    <dependency>
      <groupId>jbuilder</groupId>
      <artifactId>primetime</artifactId>
      <version>2005</version>
    </dependency>

  </dependencies>


</project>
