<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="Mevenide Project" 
	     href="http://mevenide.sf.net/" 
		 xmlns:j="jelly:core">

  <title>Mevenide</title>

  <body>
    <menu name="Home">
      <item name="Source Repositories"     href="/source-repos.html" />
    </menu>

    <menu name="Sub Projects">
    #foreach ($reactorProject in $reactorProjects)
      <j:if test="${!reactorProject.name.equals('Mevenide Master')}">
        <item name="${reactorProject.name}" href="${reactorProject.url}"/>
      </j:if>
    #end
    </menu>

  </body>
</project>

