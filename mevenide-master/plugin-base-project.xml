<?xml version="1.0" ?>
<!DOCTYPE project [
    <!ENTITY scm-reports SYSTEM "file:../mevenide-master/pom-fragments/plugin-pom-fragments/scm-reports.xml">
]>    
<project>
	
    <pomVersion>3</pomVersion>
    
    <groupId>mevenide</groupId>
    
    <logo>/images/mevenide-simple-125x37.png</logo>
    
    <organization>
      <name>The Codehaus</name>
      <url>http://www.codehaus.org/</url>
      <logo>images/codehaus_209x40.gif</logo>
    </organization>
    
    <siteAddress>beaver.codehaus.org</siteAddress>
	<siteDirectory>/home/projects/mevenide/public_html/${artifactId}</siteDirectory>
	
    <repository>
      <connection>scm:cvs:pserver:anonymous@cvs.mevenide.codehaus.org:/home/projects/mevenide/scm:${pom.artifactId}</connection>
      <developerConnection>scm:cvs:ext:${maven.username}@cvs.mevenide.codehaus.org:/home/projects/mevenide/scm:${pom.artifactId}</developerConnection>
      <url>http://cvs.mevenide.codehaus.org/cvsweb.cgi/${pom.artifactId}/</url>
    </repository>
    
	<reports>
		&scm-reports;
	</reports>
	
</project>