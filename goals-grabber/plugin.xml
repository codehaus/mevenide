<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.mevenide.goals.grabber"
   name="Mevenide Goals Grabber"
   version="0.1.1"
   provider-name="Sourceforge">

   <runtime>
	   <library name="lib/commons-collection/2.1.jar">
         	<export name="*"/>
         	<packages prefixes="org.apache.commons.collection"/>
         </library>
         <library name="mevenide-goals-grabber-0.1.jar">
         	<export name="*"/>
         	<packages prefixes="org.mevenide.goals"/>
         </library>
   </runtime>
   <requires>
      <import plugin="org.mevenide.core"/>
   </requires>


</plugin>
