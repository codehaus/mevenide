<?xml version="1.0" encoding="UTF-8"?>

<!-- code completion definition for the jelly-tag library -->

<taglib name="jelly:util">
    <tag name="available">
        <attr name="file"/>
        <attr name="uri"/>
        <attr name="trim"/>
    </tag>
    <tag name="file">
        <attr name="name"/>
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
    <tag name="loadText">
        <attr name="encoding"/>
        <attr name="file"/>
        <attr name="uri"/>
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
    <tag name="properties">
        <attr name="file"/>
        <attr name="uri"/>
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
    <tag name="replace">
        <attr name="newChar"/>
        <attr name="oldChar"/>
        <attr name="var"/>
        <attr name="value"/>
        <attr name="trim"/>
    </tag>
    <tag name="sleep">
        <attr name="milis"/>
        <attr name="trim"/>
    </tag>
    <tag name="tokenize">
        <attr name="delim"/>
        <attr name="var"/>
        <attr name="trim"/>
    </tag>
</taglib>
