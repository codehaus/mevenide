<nbm>
   <moduleType>autoload</moduleType>
   <codeNameBase>org.codehaus.mevenide.nbmvnembedder/3</codeNameBase>
   <cluster>mevenide</cluster>
   <manifest>src/main/nbm/manifest.mf</manifest>
   <homepageUrl>http://mevenide.codehaus.org/m2-site</homepageUrl>
   <distributionUrl>http://mevenide.codehaus.org/nbm_release2</distributionUrl>
   <licenseName>Apache License, Version 2.0</licenseName>
   <licenseFile>license.txt</licenseFile>
   <libraries>
      <library>org.apache.maven:maven-embedder</library>
      <library>jdom:jdom</library>
   </libraries> 
   <nbmResources>
       <nbmResource>
           <baseDirectory>src/main/nbm</baseDirectory>
           <includes>
               <include>maven2/settings.xml</include>
               <include>maven2/plugin-registry.xml</include>
               <include>maven2/rootpackage/default-report.xml</include>
               <include>maven2/fallback_pom.xml</include>
           </includes>
       </nbmResource>
   </nbmResources>
</nbm>