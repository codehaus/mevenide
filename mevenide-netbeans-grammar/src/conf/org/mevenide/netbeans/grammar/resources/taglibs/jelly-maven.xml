<?xml version="1.0" encoding="UTF-8"?>

<!-- code completion definition for the jelly-tag library -->

<taglib name="jelly:maven">
    <tag name="addPath"> 
      <attr name="id"/>
      <attr name="refid"/>
      <attr name="trim"/>
    </tag>
    <tag name="concat"> 
      <attr name="basedir"/>
      <attr name="descriptor"/>
      <attr name="goals"/>
      <attr name="ignoreFailures"/>
      <attr name="outputFile"/>
      <attr name="trim"/>
    </tag>
    <tag name="copyResources"> 
      <attr name="resources"/>
      <attr name="todir"/>
      <attr name="trim"/>
    </tag>
    <tag name="input"> 
      <attr name="basedir"/>
      <attr name="descriptor"/>
      <attr name="file"/>
      <attr name="goals"/>
      <attr name="ignoreFailures"/>
      <attr name="trim"/>
    </tag>
    <tag name="makeAbsolutePath"> 
      <attr name="basedir"/>
      <attr name="path"/>
      <attr name="var"/>
      <attr name="trim"/>
    </tag>
    <tag name="makeRelativePath"> 
      <attr name="basedir"/>
      <attr name="path"/>
      <attr name="separator"/>
      <attr name="var"/>
      <attr name="trim"/>
    </tag>
    <tag name="maven"> 
      <attr name="basedir"/>
      <attr name="descriptor"/>
      <attr name="goals"/>
      <attr name="ignoreFailures"/>
      <attr name="trim"/>
    </tag>
    <tag name="paramCheck"> 
      <attr name="fail"/>
      <attr name="message"/>
      <attr name="value"/>
      <attr name="trim"/>
    </tag>
    <tag name="pluginVar"> 
      <attr name="plugin"/>
      <attr name="property"/>
      <attr name="var"/>
      <attr name="trim"/>
    </tag>
    <tag name="pom"> 
      <attr name="projectDescriptor"/>
      <attr name="var"/>
      <attr name="trim"/>
    </tag>
    <tag name="property"> 
      <attr name="defaultValue"/>
      <attr name="name"/>
      <attr name="var"/>
      <attr name="trim"/>
    </tag>
    <tag name="reactor"> 
      <attr name="banner"/>
      <attr name="basedir"/>
      <attr name="descriptor"/>
      <attr name="excludes"/>
      <attr name="glob"/>
      <attr name="goals"/>
      <attr name="ignoreFailures"/>
      <attr name="includes"/>
      <attr name="postProcessing"/>
      <attr name="trim"/>
    </tag>
    <tag name="rootRelativePath"> 
      <attr name="path"/>
      <attr name="rootdir"/>
      <attr name="var"/>
      <attr name="trim"/>
    </tag>
    <tag name="snapshotSignature"> 
      <attr name="project"/>
      <attr name="trim"/>
    </tag>
    <tag name="userCheck"> 
      <attr name="user"/>
      <attr name="trim"/>
    </tag>
</taglib>
